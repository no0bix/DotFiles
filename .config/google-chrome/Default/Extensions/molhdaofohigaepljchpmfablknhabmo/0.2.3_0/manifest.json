{
   "content_scripts": [ {
      "css": [ "src/inject/inject.css" ],
      "matches": [ "*://*.reddit.com/*" ]
   }, {
      "css": [ "src/inject/inject-togglebutton.css" ],
      "matches": [ "*://*.reddit.com/*" ]
   }, {
      "js": [ "src/inject/jquery-2.1.1.min.js", "src/inject/webfont.js", "src/inject/inject.js", "src/inject/ga.js" ],
      "matches": [ "*://*.reddit.com/*" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.segment.com; object-src 'self'",
   "default_locale": "en",
   "description": "This extension adds a beautiful & clutter free reading mode for reddit.",
   "homepage_url": "http://www.codepen.io/saransh",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Readr for Reddit",
   "options_page": "src/options/options.html",
   "permissions": [ "*://*.reddit.com/*", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2.3",
   "web_accessible_resources": [ "src/inject/inject-tooltip.html", "src/inject/inject-readr.html", "src/inject/inject-whatsnew.html", "src/options/options.html" ]
}
